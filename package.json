{
  "name": "vite_fast",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:ts": "tsc --noEmit",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "cd .. && husky front/vue_app/.husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "msw-auto-mock": "npx msw-auto-mock __tests__/openapi.json -o ./__tests__ ",
    "build-redocly-mockdata": "npx redocly bundle document/swagger.yml --output ./src/mocks/openapi.json",
    "build-redocly-html": "npx redocly build-docs document/swagger.yml --output=document/openapi.html",
    "generate:openapi": "npx openapi-typescript document/swagger.yml -o ./src/shared/api/v1.schema.ts",
    "generate:mock": "npx msw-auto-mock document/swagger.yml -o ./src/mocks"
  },
  "lint-staged": {
    "*.{ts,vue}": "npm run format"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.13.2",
    "axios": "^1.7.7",
    "openapi-fetch": "^0.12.0",
    "pinia": "^2.2.2",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.1",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4",
    "@faker-js/faker": "^9.0.0",
    "@redocly/cli": "^1.22.1",
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/vue3": "^8.2.9",
    "@storybook/vue3-vite": "^8.2.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.5.4",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.56.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.5",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "msw": "^2.4.2",
    "msw-auto-mock": "^0.25.0",
    "msw-storybook-addon": "^2.0.3",
    "npm-check-updates": "^17.1.1",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^7.4.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
