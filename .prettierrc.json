{
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": false,
  "printWidth": 100,
  "trailingComma": "all",
  "importOrder": [
    "^vue",
    "^[a-z]\\w",
    "^@\\w",
    "^@/\\w",
    "^[./]"
  ]
}